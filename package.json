{
  "name": "react-redux-test",
  "description": "practice of react+redux+test frameworks+webpack and maybe express.js :D",
  "version": "0.0.1",
  "main": "app/main.jsx",
  "scripts": {
    "lint": "eslint \"app/**/*.@(js|jsx)\"",
    "test": "mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch --require ./test/setup.js",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --inline --colors --content-base app",
    "server": "node server.js"
  },
  "dependencies": {
    "react": "~0.14.6",
    "react-dom": "~0.14.6",
    "redux": "~3.3.1",
    "react-redux": "~4.4.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "express": "~4.13.4",
    "cors": "*",
    "body-parser": "*",
    "lodash": "~4.6.1",
    "es6-promise": "~3.1.2",
    "isomorphic-fetch": "~2.2.1",
    "webpack-dev-middleware": "~1.5.1",
    "webpack-hot-middleware": "~2.9.1"
  },
  "devDependencies": {
    "webpack": "*",
    "webpack-dev-server": "*",
    "open-browser-webpack-plugin": "0.0.1",
    "copy-webpack-plugin": "~0.3.3",
    "css-loader": "~0.23.0",
    "style-loader": "~0.13.0",

    "babel-core": "~6.2.1",
    "babel-eslint": "~4.1.6",
    "babel-loader": "~6.2.0",
    "babel-plugin-transform-runtime": "~6.1.18",
    "babel-preset-es2015": "~6.1.18",
    "babel-preset-react": "~6.1.18",
    "babel-preset-stage-0": "~6.1.18",

    "chai": "^3.4.1",
    "enzyme": "^1.4.1",
    "mocha": "^2.3.4",
    "jsdom": "^7.2.2",
    "jsx-test": "^0.8.2",
    "react-addons-test-utils": "^0.14.6",
    "nock": "7.4.0",
    "redux-mock-store": "^0.0.6",

    "eslint": "~1.10.1",
    "eslint-config-airbnb": "~1.0.0",
    "eslint-plugin-react": "~3.10.0",

    "object-assign": "^4.0.1",
    "precommit-hook": "~3.0.0"
  },
  "author": "OnionCoder"
}
